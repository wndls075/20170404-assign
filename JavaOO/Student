public class Student {
	
	private String dm;
	 
	 public String getdm(){//getter
		 return dm;
	 }
	 
	 public void setdm(String dm){//setter
			this.dm = dm;
		}
	 
	 private int scnum;
	 
	 public int getscnum(){//getter
		 return scnum;
	 }
	 
	 public void setscnum(int scnum){//setter
			this.scnum = scnum;
		}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Student student = new Student();
		
		student.setdm("컴퓨터정보공학과");
		student.setscnum(20141642);
		
		System.out.println("학과 : " + student.getdm());
		System.out.println("학번 : " + student.getscnum());
		
	}

}
